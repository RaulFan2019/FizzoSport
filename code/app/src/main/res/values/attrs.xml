<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WheelIndicatorView">
        <attr name="itemsLineWidth" format="dimension" />
        <attr name="filledPercent" format="integer" />
        <attr name="backgroundColor" format="color" />
        <attr name="innerBackgroundCircleLineWidth" format="dimension"/>
        <attr name="src" format="integer"/>
    </declare-styleable>

    <!-- data pick -->
    <declare-styleable name="NumberPicker">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="float|dimension" />
        <attr name="startNumber" format="integer" />
        <attr name="endNumber" format="integer" />
        <attr name="currentNumber" format="integer" />
        <attr name="verticalSpacing" format="dimension" />
        <attr name="flagText" format="string|reference" />
        <attr name="flagTextSize" format="dimension" />
        <attr name="flagTextColor" format="color" />
        <attr name="np_backgroundColor" format="color" />
        <attr name="rowNumber" format="integer" />
    </declare-styleable>
</resources>