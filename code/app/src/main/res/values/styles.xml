    </style>

    <!-- Main info detail list item -->
    <!--  标题 TIP item-->
    <style name="ItemLineTotalTitleTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/tv_item_total_title</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="ItemLineTotalTitleLeftTv" parent="ItemLineTotalTitleTv">
        <item name="android:layout_marginStart">17dp</item>
    </style>

    <style name="ItemLineTotalTitleRightTv" parent="ItemLineTotalTitleTv">
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>

    <!--  数据布局 item -->
    <style name="ItemLineNormalLeftDateTv">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">17dp</item>
        <item name="android:textColor">@color/tv_item_week_normal</item>
        <item name="android:textSize">@dimen/tv_tip_normal</item>
    </style>

    <style name="ItemLineNormalLeftValueTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15.7dp</item>
        <item name="android:textColor">@color/tv_item_value_normal</item>
    </style>

    <style name="ItemLineNormalLeftUnitTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/tv_tip_normal</item>
        <item name="android:textColor">@color/tv_item_unit_normal</item>
    </style>

    <style name="ItemLineNormalRightDoneImg">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:background">@drawable/ic_done</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!--  头部Head chart item-->
    <style name="ItemLineHeadTipImg">
        <item name="android:layout_width">33dp</item>
        <item name="android:layout_height">33dp</item>
        <item name="android:layout_marginStart">16dp</item>
    </style>

    <style name="ItemLineHeadTipTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
    </style>

    <!-- 添加数据 -->

    <style name="GuideLineAddRecordTipTv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginTop">13dp</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">@color/tv_mostly</item>
    </style>

    <style name="GuideLineAddRecordValueLl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
