        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/tv_main_right_text</item>
        <item name="android:textSize">@dimen/tv_form_value</item>
    </style>

    <style name="GuideLineRightSwitch">
        <item name="android:layout_width">51dp</item>
        <item name="android:layout_height">33dp</item>
        <item name="android:layout_alignParentEnd">true</item>
