<resources>

    <!-- login edit layout -->
    <style name="EditLayout_Frame">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
    </style>

    <style name="EditLayout_Tip">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/tv_tip_normal</item>
        <item name="android:paddingLeft">36dp</item>
        <item name="android:textSize">@dimen/tv_tip_normal</item>
    </style>

    <style name="EditLayout_ErrorTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/tv_error</item>
        <item name="android:paddingRight">36dp</item>
        <item name="android:textSize">@dimen/tv_tip_normal</item>
        <item name="android:layout_gravity">right</item>
    </style>

    <style name="EditLayout_Bg_Image">
        <item name="android:layout_width">318dp</item>
        <item name="android:layout_height">78dp</item>
        <item name="android:src">@drawable/bg_layout_et</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="EditLayout_ET">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColorHint">@color/et_hint</item>
        <item name="android:textColor">@color/et_input</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">15dp</item>
        <item name="android:paddingLeft">36dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_marginTop">4dp</item>
    </style>

    <style name="EditLayout_ET_Phone" parent="EditLayout_ET">
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">13</item>
    </style>

    <style name="EditLayout_ET_PWD" parent="EditLayout_ET">
        <item name="android:inputType">textPassword</item>
        <item name="android:maxLength">16</item>
    </style>

    <!-- Commit Button -->
    <style name="Btn_Commit">
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">16dp</item>
        <item name="android:layout_width">288dp</item>
        <item name="android:textColor">@color/btn_mostly_tv</item>
        <item name="android:background">@drawable/btn_commit_mostly</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="Btn_Commit_transparent">
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">16dp</item>
        <item name="android:layout_width">288dp</item>
        <item name="android:textColor">@color/btn_white_tv</item>
        <item name="android:background">@drawable/bg_btn_transparent</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <!-- indicator -->
    <style name="CirclePageIndicator" parent="@android:style/Widget">
        <item name="centered">@bool/default_circle_indicator_centered</item>
        <item name="fillColor">@color/default_circle_indicator_fill_color</item>
        <item name="orientation">@integer/default_circle_indicator_orientation</item>
        <item name="radius">@dimen/default_circle_indicator_radius</item>
        <item name="snap">@bool/default_circle_indicator_snap</item>
        <item name="strokeColor">@color/default_circle_indicator_stroke_color</item>
        <item name="strokeWidth">@dimen/default_circle_indicator_stroke_width</item>
    </style>

    <!-- Divider -->
    <style name="DividerH">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/bg_divider</item>
    </style>

    <style name="DividerV">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/bg_divider</item>
    </style>

    <style name="DividerHSporting" parent="DividerH">
        <item name="android:background">#476071</item>
    </style>
    <style name="DividerVSporting" parent="DividerV">
        <item name="android:background">#476071</item>
    </style>
    <!-- Dialog -->
    <!--<style name="AutoCloseDialogTitle" parent="android:style/TextAppearance.DialogWindowTitle">-->
    <!--<item name="android:gravity">left</item>-->
    <!--</style>-->

    <style name="DialogWindowTitle">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/primary</item>
    </style>

    <style name="DialogWindowTitle.DeviceDefault">
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:textAppearance">@style/DialogWindowTitle</item>
    </style>

    <style name="DialogTheme" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:windowTitleStyle">@style/DialogWindowTitle.DeviceDefault</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="ShareDialogTheme" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:windowTitleStyle">@style/DialogWindowTitle.DeviceDefault</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <!-- Title -->
    <style name="Title_Layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Title_BackButton">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:text">@string/back</item>
        <item name="android:textSize">@dimen/tv_button_title</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:drawableLeft">@drawable/ic_back</item>
        <item name="android:drawablePadding">7.5dp</item>
        <item name="android:background">?android:attr/actionBarItemBackground</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="TitleRightBtnLy">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Title_RightButton">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:background">?android:attr/actionBarItemBackground</item>
    </style>

    <style name="Title_TitleTv">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- Tab -->
    <style name="TabRootLayout">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="TabRootLayout_TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:textColor">@color/tv_tab_normal</item>

    </style>

    <style name="TabRootLayout_ImageView">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>


    <!-- main health -->
    <style name="MainHealthStatisticsTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/tv_normal</item>
        <item name="android:textSize">15.7dp</item>
    </style>


    <!-- popupWindow上下划入设置 -->
    <style name="anim_popup" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/anim_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/anim_push_bottom_out</item>
    </style>

    <!-- main user info -->
