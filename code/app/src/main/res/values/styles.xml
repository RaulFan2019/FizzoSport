        <item name="android:textColor">#2e2e2e</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <!-- Monitor list head -->
    <style name="MonitorListHeadWeekTv">
        <item name="android:layout_width">17.2dp</item>
        <item name="android:layout_height">19.1dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">11.7dp</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
    </style>

    <style name="MonitorListHeadSpaceV">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="MonitorListHeadLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>
    <style name="MonitorListHeadLineTv">
        <item name="android:layout_width">25.9dp</item>
        <item name="android:layout_height">15.3dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#fefeff</item>
        <item name="android:textSize">11.7dp</item>
